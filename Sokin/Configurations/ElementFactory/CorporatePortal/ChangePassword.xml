<?xml version="1.0" encoding="utf-8" ?>
<ChangePassword>

  <XPaths>
    <Element keyword="CorporateIdentity" locator="//*[@id='corporateIdentity']"/>
    <Element keyword="CorporateUsername" locator="//*[@id='userIdentity']"/>
    <Element keyword="CorporatePassword" locator="//*[@id='securityToken']"/>
    <Element keyword="CorporateLoginButton" locator="//*[@id='mainLoginBtn']"/>
    <Element keyword="CorporateLoginErrorButton" locator="//*[@id='cancelBtn']"/>
