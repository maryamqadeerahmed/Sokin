    <Element keyword="ConfirmPasswordField" locator="//*[@id='confirmPassword']"/>
    <Element keyword="ChangePasswordSubmitButton" locator="//*[@id='mainChangePasswordBtn']"/>
    <Element keyword="PasswordPolicyErrorIcons" locator="//*//div[@id='passwordPolicyWrapper']//*[contains(@style,'color: red')]"/>
